<Window x:Class="AForgeViewer.dialogs.RGBInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RGBInputDialog" Height="200" Width="300" ResizeMode="NoResize" SizeToContent="WidthAndHeight" ShowInTaskbar="False" Topmost="True" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid>
        <Border Margin="10">
            <Grid>
                <Label Content="Red:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <TextBox x:Name="tbRed" Height="23" Margin="74,3,0,0" TextWrapping="Wrap" Text="0.2125" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200"/>
                <Label Content="Green:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,28,0,0"/>
                <TextBox x:Name="tbGreen" Height="23" Margin="74,31,0,0" TextWrapping="Wrap" Text="0.7154" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200"/>
                <Label Content="Blue:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,59,0,0"/>
                <TextBox x:Name="tbBlue" Height="23" Margin="74,59,0,0" TextWrapping="Wrap" Text="0.0721" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200"/>
                <Button Content="Ok" HorizontalAlignment="Left" Margin="97,99,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_Ok"/>
                <Button Content="Cancel" HorizontalAlignment="Left" Margin="189,99,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_Cancel"/>
            </Grid>
        </Border>
    </Grid>
</Window>
